rule ModelTo1000Html 
	transform m : Model {
	template : "Generate-Html-Model2class.egl"
	parameters : Map{"package"="syncregions"}
	target : "gen-1000-Components/" + m.name + ".html"	
}
rule ComponentTo1000Html
	transform c : Component {
	template : "Generate-Html-Component2class.egl"
	target : "gen-1000-Components/" + c.name + '.html'	
}