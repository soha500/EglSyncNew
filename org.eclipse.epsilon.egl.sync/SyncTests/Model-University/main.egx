
rule ModuleToHTML

	transform m : Module {

	template : "Module2html.egl"

	target :    "modules/" + m.name + '.html' 
	//target :    m.name + '.html' 
	
}

rule StudentToHTML

	transform s : Student {

	template : "Student2html.egl"

	target :    "students/" + s.reg + '.html' 
	//target :     s.reg + '.html' 
	
}