rule ModelTo200Html 
	transform m : Model {
	template : "ThreeSyncs-Model2class.egl"
	parameters : Map{"package"="syncregions"}
	target : "gen-1000-components/" + m.name + '.html'
}

rule ComponentTo200Html
	transform c : Component {
	template : "ThreeSyncs-ComponentToClass.egl"
	target : "gen-1000-components/" + c.name + '.html'	
}